<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.Lottie</id>
    <version>1.1.102</version>
    <title>Lottie Animations</title>
    <projectUrl>https://github.com/Geeksltd/Zebble.Lottie</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.Lottie/master/Shared/NuGet/Icon.png</iconUrl>
    <copyright>Copyright © 2019 Geeks Ltd - All rights reserved.</copyright>
    <description>A plugin to render Adobe AfterEffects animation in Zebble apps</description>
    <authors>Geeks Ltd</authors>
    <owners>Geeks Ltd</owners>
    <dependencies>
      <group targetFramework="MonoAndroid10">
        <dependency id="Com.Airbnb.Android.Lottie" version="2.5.0" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Com.Airbnb.iOS.Lottie" version="2.5.0" />
      </group>
      <group targetFramework="UAP10"></group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\lib\Zebble.Lottie.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\lib\Zebble.Lottie.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\bin\ARM\Release\Zebble.Lottie.UWP.*" target="runtimes\win10-arm\lib\uap10.0" />
    <file src="..\..\lib\bin\x64\Release\Zebble.Lottie.UWP.*" target="runtimes\win10-x64\lib\uap10.0" />
    <file src="..\..\lib\bin\x86\Release\Zebble.Lottie.UWP.*" target="runtimes\win10-x86\lib\uap10.0" />
    <file src="..\..\lib\bin\Release\Zebble.Lottie.UWP.*" target="ref\uap10.0" />
  </files>
</package>